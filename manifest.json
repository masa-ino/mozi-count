{
  "name": "文字数カウント",
  "version": "1.0",
  "description": "文字を入力すると文字数をカウントできます",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "count.html",
    "default_icon": {
      "32": "images/icon.png"
    }
  },
  "icons": {
    "48": "images/icon.png"
  },
  "manifest_version": 2
}


